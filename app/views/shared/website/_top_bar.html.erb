      <li>
        <a  <% if params[:controller] == 'look_raw_material' %> class="active" <% end %> href="/look_raw_material/index.html">找原料</a>
      </li>
      <li>
        <a href="/firm_orders/index.html" class="hot <%= params[:controller] == 'firm_orders' ? 'active' : '' %>">实单竞标
