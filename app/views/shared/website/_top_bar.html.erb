            <a href="/">
              <img src="/assets/website/logo4.jpg">
            </a>
          </div>
          <%= render '/shared/website/quick_search' %>
        </div>
    </div>
<% else %>
    <div class="headWrapper">
      <div class="topLogo">
        <a href="/">
          <img src="/assets/website/home_logo.png">
        </a>
      </div>
      <%= render '/shared/website/quick_search' %>
    </div>
<% end  %>

<div class="navWrapper">
