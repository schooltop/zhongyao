<!-- 右侧工具栏 -->

<div class="toolbar J_right_toolbar">
  <% if current_user.present? && params[:controller] != 'home' %>
      <a href="<%= my_whmall_home_index_path %>"><img src="/assets/website/common/mine.png" alt="" width="54px;"></a>
  <% end %>

  <div class="wrap">
    <a href="/look_raw_material/publish_inquiry.html?siteId=2"><i class="icon_FREE J_icon_FREE"></i></a>
    <a href="<%= reagent_mall_shop_carts_path %>" class="icon_shop_cart J_shop_cart">
      <span class="num">
        <% if current_user.blank? %>
          <%= ShoppingCart.actived.where(session_id: session.id).count %>
        <% else %>
            <%= ShoppingCart.actived.where(user_id: current_user.id).count %>
        <% end %>
      </span>
    </a>
    <a href="/about_us/buy_novice_guide.html"><i class="icon_guidelines"></i></a>
    <div class="online_service">
      <i class="icon_online_service"></i>
      <div class="details_online_service annotation transition hidden">
        <div class="ovh">
          <i class="icon_service_qq"></i>
          <p>客服QQ：800030582</p>
          <p class="time">工作日：09:00-17:30</p>
          <div class="ml40"><div class="J_wapQQ" style="width:92px; height:22px;background-repeat: no-repeat;cursor: pointer; background-image: url(data:image/png;base64,);" onclick="window.open('http://wpa.b.qq.com/cgi/wpa.php?ln=1&key=XzgwMDAzMDU4Ml80ODUwMjNfODAwMDMwNTgyXzJf')"></div>
          </div>
        </div>
        <div class="ovh mt30">
          <i class="icon_service_phone"></i>
          <p>服务热线：4008-200-497</p>
          <p class="time">工作日：09:00-17:30</p>
        </div>
      </div>
    </div>
    <a href="javascript:void(0)" class="feedback" id="advance_feedback">
      <i class="icon_feedback"></i>
    </a>
  </div>
  <div class="icon_gohead J_gotop"></div>
</div>



<!-- 提交反馈 -->
<div class="dimmer" id="J_advance_dialog" >
  <div class="ks-dialog ks-dialog3" aria-hidden="false"  style="width: 453px;height:auto;left: 50%;top: 50%;margin-left: -225px;margin-top: -175px;" >
    <a href="javascript:;" class="ks-dialog-close ks-overlay-close" role="button">
      <span class="ks-overlay-close-x">x</span>
    </a>
    <div class="ks-dialog-header">
      <h4 class="tsl">提交反馈</h4>
    </div>
    <div id="customer_bug_before" >
      <%= simple_form_for CustomerBug.new, url: add_customer_bug_home_index_path , method: :post, html: {class: '',id: 'add_customer_bug_form'} do |f| %>
        <div class="feedback_form" style="margin-left: 20px">
          <table>
            <tbody>
            <tr valign="top">
              <td>
                <p style=' font-size: 12px;color: #ff9600;line-height: 24px;margin-left: 0;margin-top: 6px;'>您好，期待您的意见和建议！</p>
                <textarea class="mt10" placeholder="如果您在使用过程中有什么不好用的，或者对我们的服务有什么不满意的地方，请大声说出来！ 我们会每天关注您的反馈，不断地优化产品和服务，努力做到最好！"  maxlength="200" style="width: 383px"  id="customer_bug_content" name="customer_bug[content]"></textarea>
              </td>
            </tr>

            <tr>
              <td>
                <% if current_user.blank? %>
                    <p style="margin-left:0px" class="title">
                      联系方式
                    </p>
                <% end %>
                <input type="text" id='customer_bug_contact_style' name='customer_bug[contact_style]'  <% if current_user.present? %> style="display: none "  <% else %>style="margin-left: 0px;width: 408px"<% end %>  placeholder="请输入您的常用邮箱或者联系电话,以便更好的联系到您." class="input" value="<%= current_user&.email %>">
              </td>
            </tr>
            <tr valign="top">
              <td>
                <p style='font-size: 12px;color: red;line-height: 24px;margin-left: 0;margin-top: 6px;' id="customer_bug_content_err" ></p>
              </td>
            </tr>

            </tbody></table>
        </div>
      <% end %>
      <div class=" ks-footer mr5">
        <a href="javascript:void(0);" class="btn_default_min " onclick="$(this).parents('.dimmer').hide()">取消</a>
        <a href="javascript:void(0);" class="btn_submit_min" id="add_customer_bug_form_btn">提交</a>
      </div>
    </div>
    <div id="customer_bug_after" style="display: none" >
      <div class="feedback_form" style="margin-left: 20px">
        <div class="feedback_error_details" >
          <p class="mt10"><span>反馈内容：</span></p>
          <p id="J_bug_contant"></p>

          <div class="status">感谢您的反馈，我们会不断地优化产品和服务，努力做到最好！</div>
        </div>

      </div>
      <div class=" ks-footer mr5">
        <a href="javascript:void(0);" class="btn_submit_min" onclick="$(this).parents('.dimmer').hide()" >确定</a>
      </div>
    </div>

  </div>
</div>
