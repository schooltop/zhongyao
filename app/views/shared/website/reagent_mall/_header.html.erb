          <span class="mr10" id="topbar_greet">您好! 欢迎来网化商城</span>
          <a href="<%= user_session_path %>" class="login">请登录</a>
          <a href="<%= new_user_registration_path %>" class="item">免费注册</a>
      <% else %>
          <span class="mr10" id="topbar_greet"><%= current_user.name %>，欢迎来网化商城</span>
          <a href="<%= my_whmall_home_index_path %>" class="item">会员中心</a>
          <%= link_to destroy_user_session_path ,method: 'delete', class: 'login' do %>
              登出
          <% end %>
      <% end %>
    </div>
    <div class="topRight">
      <a href="<%= reagent_mall_shop_carts_path %>" class="shopp_cart">购物车 <span class="num"><%= @my_cart_count.to_i %></span></a>
      <a href="/look_raw_material/publish_inquiry.html?siteId=1" class="icon_post_buying"></a>
      <a href="/product_release.html" class="icon_sell_products" target="_blank"></a>
      <div class="icon_weixin J_not_transformation">
        <img src="/assets/website/icons/weixin3.png" alt="" >
      </div>
      <div class="icon_weibo">
        <img src="/assets/website/icons/weibo3.png" alt="">
      </div>
    </div>
  </div>
</div>